import csv
from matplotlib import pyplot as plt
from datetime import datetime

file='sitka_weather_07-2014.csv'
with open(file) as it :
	reader=csv.reader(it)
	data=next(reader)

	dates,highs,lows=[],[],[]

	for ras in reader:
		high=int(ras[1])
		low=int(ras[2])
		date=datetime.strptime(ras[0],"%Y-%m-%d")
		highs.append(high)
		lows.append(low)
		dates.append(date)


for line in range(31):
	print(dates[line])
	print(highs[line])
	print(lows[line])

fig=plt.figure(dpi=128)
plt.plot(dates,highs,c='red')
plt.plot(dates,lows,c='blue')
plt.fill_between(dates,highs,lows,facecolor='blue',alpha=0.4)
fig.autofmt_xdate()
plt.show()
